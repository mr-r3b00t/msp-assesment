              "options": ["Yes", "No"],
              "required": true
            },
            {
              "number": 39,
              "text": "Are there different monitoring policies for high-privileged users compared to low-privileged users?",
              "type": "radio",
              "options": ["Yes", "No"],
              "required": true,
              "conditional": {
                "trigger": "Yes",
                "fields": [
                  {
                    "text": "Describe the differences in monitoring policies",
                    "type": "textarea",
                    "required": true
                  }
                ]
              }
            }
